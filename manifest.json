{
  "background": {
    "page": "popup.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
      "19": "images/logo.png",
      "38": "images/logo.png"
    },
    "default_popup": "popup.html",
    "default_title": "In-place JSON Formatter"
  },
  "description": "This plugin gives you popup on your chrome browser which can be used to validate and format your JSON.",
  "icons": {
    "128": "images/logo.png",
    "256": "images/logo.png",
    "16": "images/logo.png",
    "48": "images/logo.png"
  },
  "manifest_version": 2,
  "name": "In-place JSON Formatter",
  "short_name": "In-place JSON Formatter",
  "omnibox": {
    "keyword": "jf"
  },
  "permissions": [
    "clipboardWrite" , "tabs"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.0.0"
}
